<?xml version="1.0" encoding="utf-8"?><!--suppress AndroidDomInspection -->
<navigation
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/navigationGraph"
  >

  <action
    android:id="@+id/actionNavigateProgressFragment"
    app:destination="@id/progressFragment"
    app:launchSingleTop="true"
    app:popUpTo="@id/navigationGraph"
    app:popUpToInclusive="true"
    />

  <action
    android:id="@+id/actionNavigateProgressMoviesFragment"
    app:destination="@id/progressMoviesFragment"
    app:launchSingleTop="true"
    app:popUpTo="@id/navigationGraph"
    app:popUpToInclusive="true"
    />

  <action
    android:id="@+id/actionNavigateDiscoverFragment"
    app:destination="@id/discoverFragment"
    app:launchSingleTop="true"
    app:popUpTo="@id/navigationGraph"
    app:popUpToInclusive="true"
    />

  <action
    android:id="@+id/actionNavigateDiscoverMoviesFragment"
    app:destination="@id/discoverMoviesFragment"
    app:launchSingleTop="true"
    app:popUpTo="@id/navigationGraph"
    app:popUpToInclusive="true"
    />

  <action
    android:id="@+id/actionNavigateFollowedShowsFragment"
    app:destination="@id/followedShowsFragment"
    app:launchSingleTop="true"
    app:popUpTo="@id/navigationGraph"
    app:popUpToInclusive="true"
    />

  <action
    android:id="@+id/actionNavigateFollowedMoviesFragment"
    app:destination="@id/followedMoviesFragment"
    app:launchSingleTop="true"
    app:popUpTo="@id/navigationGraph"
    app:popUpToInclusive="true"
    />

  <fragment
    android:id="@+id/discoverFragment"
    android:name="com.michaldrabik.ui_discover.DiscoverFragment"
    android:label="fragmentDiscover"
    >
    <action
      android:id="@+id/actionDiscoverFragmentToShowDetailsFragment"
      app:destination="@id/showDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionDiscoverFragmentToSearchFragment"
      app:destination="@id/searchFragment"
      app:launchSingleTop="true"
      />
    <action
      android:id="@+id/actionDiscoverFragmentToSettingsFragment"
      app:destination="@id/settingsFragment"
      app:launchSingleTop="true"
      />
  </fragment>

  <fragment
    android:id="@+id/discoverMoviesFragment"
    android:name="com.michaldrabik.ui_discover_movies.DiscoverMoviesFragment"
    android:label="fragmentDiscoverMovies"
    >
    <action
      android:id="@+id/actionDiscoverMoviesFragmentToMovieDetailsFragment"
      app:destination="@id/movieDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionDiscoverMoviesFragmentToSearchFragment"
      app:destination="@id/searchFragment"
      app:launchSingleTop="true"
      />
    <action
      android:id="@+id/actionDiscoverMoviesFragmentToSettingsFragment"
      app:destination="@id/settingsFragment"
      app:launchSingleTop="true"
      />
  </fragment>

  <fragment
    android:id="@+id/progressFragment"
    android:name="com.michaldrabik.ui_progress.main.ProgressFragment"
    android:label="fragmentProgressMain"
    >
    <action
      android:id="@+id/actionProgressFragmentToShowDetailsFragment"
      app:destination="@id/showDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionProgressFragmentToMovieDetailsFragment"
      app:destination="@id/movieDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionProgressFragmentToSettingsFragment"
      app:destination="@id/settingsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionProgressFragmentToTraktSyncFragment"
      app:destination="@id/traktSyncFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:exitAnim="@anim/anim_fade_out"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/progressMoviesFragment"
    android:name="com.michaldrabik.ui_progress_movies.main.ProgressMoviesFragment"
    android:label="fragmentProgressMain"
    >
    <action
      android:id="@+id/actionProgressMoviesFragmentToMovieDetailsFragment"
      app:destination="@id/movieDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionProgressMoviesFragmentToShowDetailsFragment"
      app:destination="@id/showDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionProgressMoviesFragmentToSettingsFragment"
      app:destination="@id/settingsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionProgressMoviesFragmentToTraktSyncFragment"
      app:destination="@id/traktSyncFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:exitAnim="@anim/anim_fade_out"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/showDetailsFragment"
    android:name="com.michaldrabik.ui_show.ShowDetailsFragment"
    android:label="fragmentShowDetails"
    >
    <action
      android:id="@+id/actionShowDetailsFragmentToSelf"
      app:destination="@id/showDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:exitAnim="@anim/anim_fade_out"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionShowDetailsFragmentToFanartGallery"
      app:destination="@id/fanartGalleryFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:exitAnim="@anim/anim_fade_out"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/movieDetailsFragment"
    android:name="com.michaldrabik.ui_movie.MovieDetailsFragment"
    android:label="fragmentMovieDetails"
    >
    <action
      android:id="@+id/actionMovieDetailsFragmentToSelf"
      app:destination="@id/movieDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:exitAnim="@anim/anim_fade_out"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionMovieDetailsFragmentToFanartGallery"
      app:destination="@id/fanartGalleryFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:exitAnim="@anim/anim_fade_out"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/fanartGalleryFragment"
    android:name="com.michaldrabik.ui_gallery.FanartGalleryFragment"
    android:label="fragmentFanartGallery"
    />

  <fragment
    android:id="@+id/followedShowsFragment"
    android:name="com.michaldrabik.ui_my_shows.main.FollowedShowsFragment"
    android:label="fragmentFollowedShows"
    >
    <action
      android:id="@+id/actionFollowedShowsFragmentToShowDetailsFragment"
      app:destination="@id/showDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionFollowedShowsFragmentToSettingsFragment"
      app:destination="@id/settingsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionFollowedShowsFragmentToStatisticsFragment"
      app:destination="@id/statisticsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/followedMoviesFragment"
    android:name="com.michaldrabik.ui_my_movies.main.FollowedMoviesFragment"
    android:label="fragmentFollowedMovies"
    >
    <action
      android:id="@+id/actionFollowedMoviesFragmentToMovieDetailsFragment"
      app:destination="@id/movieDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionFollowedMoviesFragmentToSettingsFragment"
      app:destination="@id/settingsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionFollowedMoviesFragmentToStatisticsFragment"
      app:destination="@id/statisticsMoviesFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/searchFragment"
    android:name="com.michaldrabik.ui_search.SearchFragment"
    android:label="fragmentSearch"
    >
    <action
      android:id="@+id/actionSearchFragmentToShowDetailsFragment"
      app:destination="@id/showDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
    <action
      android:id="@+id/actionSearchFragmentToMovieDetailsFragment"
      app:destination="@id/movieDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/settingsFragment"
    android:name="com.michaldrabik.ui_settings.SettingsFragment"
    android:label="fragmentSettings"
    >
    <action
      android:id="@+id/actionSettingsFragmentToTraktSync"
      app:destination="@id/traktSyncFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:exitAnim="@anim/anim_fade_out"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/statisticsFragment"
    android:name="com.michaldrabik.ui_statistics.StatisticsFragment"
    android:label="fragmentStatistics"
    >
    <action
      android:id="@+id/actionStatisticsFragmentToShowDetailsFragment"
      app:destination="@id/showDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/statisticsMoviesFragment"
    android:name="com.michaldrabik.ui_statistics_movies.StatisticsMoviesFragment"
    android:label="fragmentStatisticsMovies"
    >
    <action
      android:id="@+id/actionStatisticsMoviesFragmentToMovieDetailsFragment"
      app:destination="@id/movieDetailsFragment"
      app:enterAnim="@anim/anim_fade_in"
      app:launchSingleTop="true"
      app:popExitAnim="@anim/anim_fade_out"
      />
  </fragment>

  <fragment
    android:id="@+id/traktSyncFragment"
    android:name="com.michaldrabik.ui_trakt_sync.TraktSyncFragment"
    android:label="fragmentTraktSync"
    />

</navigation>